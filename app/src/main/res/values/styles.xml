<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="roundedImageView">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="CustomChipRed" parent="Widget.MaterialComponents.Chip.Entry">
        <item name="chipBackgroundColor">@drawable/chip_interest_red</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:padding">8dp</item>
    </style>

    <style name="CustomChipGrey" parent="Widget.MaterialComponents.Chip.Entry">
        <item name="chipBackgroundColor">@drawable/chip_interest_grey</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:padding">8dp</item>
    </style>
</resources>